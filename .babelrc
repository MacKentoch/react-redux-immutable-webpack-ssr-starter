{
  "presets": [
    ["es2015"],
    "react",
    "stage-2",
    "flow"
  ],
  "plugins": [
    "react-hot-loader/babel",
    "transform-regenerator",
    "transform-async-to-generator"
  ],
  "env": {
    "production": {
      "presets": [
        "react-optimize"
      ],
      "comments": false
    },
    "server": {
      "presets": [
        "react-optimize",
        "babili"
      ],
      "comments": false
    }
  }
}
